{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		// Existing
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		// Added
		"composite": true,
		"emitDeclarationOnly": true,
		"noEmit": false
	}
}
