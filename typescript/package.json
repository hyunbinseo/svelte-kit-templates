		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.1",
		"vite": "^5.4.0"
